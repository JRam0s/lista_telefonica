<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="548.0" prefWidth="382.0" spacing="5.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ifpr.pgua.eic.listatelefonica.controllers.JanelaLista">
   <children>
      <VBox prefHeight="380.0" prefWidth="198.0" spacing="5.0">
         <children>
            <ListView fx:id="ltvContatos" onMouseClicked="#mostraDetalhes" prefHeight="200.0" prefWidth="150.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </ListView>
            <Button mnemonicParsing="false" onMouseClicked="#voltar" text="Voltar" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </padding>
      </VBox>
      <TextArea fx:id="taDetalhes" prefHeight="336.0" prefWidth="224.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
